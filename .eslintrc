{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}